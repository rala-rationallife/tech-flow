---
import Layout from "@layouts/layout.astro";
---

<Layout>
  <div class="container">
    <h1>Tech Flow.</h1>
  </div>
</Layout>

<style is:global>
  main {
    display: grid;
    grid-template-columns: subgrid;
  }
</style>

<style>
  .container {
    grid-column: 2 / -2;
    display: grid;
    place-content: center;
  }
</style>
